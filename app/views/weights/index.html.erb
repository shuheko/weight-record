<div class="contents row">
  <% @weights.each do |weight| %>
    <div class="date">
      <p><%= weight.date %>日目</p>
    </div>
    <div class="value">
      <p><%= weight.value %>kg</p>

    </div>
    <div class="destroy">
      <%= link_to '　削除', weight_path(weight.id), method: :delete %>
    </div>
  <% end %>
</div>
